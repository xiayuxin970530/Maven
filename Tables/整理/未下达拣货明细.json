  "ExternalDatabaseInfo": {
    "ConnectionId": "pld",
    "SourceTableName": "未下达拣货明细",
    "SourceTableSchema": "dbo",
    "CreateViewSql": "SELECT  CAST(a.PICKID AS nvarchar) + CAST(a.dlcinvcod AS nvarchar) + CAST(a.cwhcode AS nvarchar) \r\n                   + CAST(a.ztcode AS nvarchar) AS 标识, a.PICKID, a.dlcinvcod, CAST(a.拣货总数 AS decimal(18, 4)) AS 拣货总数, \r\n                   a.cwhcode, '发货单编码' AS cDLCode, '客户编码' AS 客户编码, a.code, a.carcode, a.pdate, '客户名称' AS 客户名称, \r\n                   1 AS 批次计数, 1 AS 货位计数, c.已下达任务, CAST(a.拣货总数 - ISNULL(c.已下达任务, 0) AS decimal(18, 4)) \r\n                   AS 未下达余额, a.ztcode\r\nFROM      dbo.拣货汇总 AS a LEFT OUTER JOIN\r\n                   dbo.任务绑定汇总 AS c ON a.标识 = c.标识\r\nWHERE   (CAST(a.拣货总数 - ISNULL(c.已下达任务, 0) AS decimal(18, 4)) > 0)\r\nunion\r\nSELECT  CAST(a.PICKID AS nvarchar) + CAST(a.dlcinvcod AS nvarchar) + CAST(a.cwhcode AS nvarchar) \r\n                   + CAST(a.ztcode AS nvarchar) AS 标识, a.PICKID, a.dlcinvcod, CAST(a.拣货总数 AS decimal(18, 4)) AS 拣货总数, \r\n                   a.cwhcode, '发货单编码' AS cDLCode, '客户编码' AS 客户编码, a.code, a.carcode, a.pdate, '客户名称' AS 客户名称, \r\n                   1 AS 批次计数, 1 AS 货位计数, c.已下达任务, CAST(a.拣货总数 - ISNULL(c.已下达任务, 0) AS decimal(18, 4)) \r\n                   AS 未下达余额, a.ztcode\r\nFROM      dbo.拣货汇总zt700 AS a LEFT OUTER JOIN\r\n                   dbo.任务绑定汇总 AS c ON a.标识 = c.标识\r\nWHERE   (CAST(a.拣货总数 - ISNULL(c.已下达任务, 0) AS decimal(18, 4)) > 0)",
