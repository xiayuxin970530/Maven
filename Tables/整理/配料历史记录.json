  "ExternalDatabaseInfo": {
    "ConnectionId": "pld",
    "SourceTableName": "配料历史记录",
    "SourceTableSchema": "dbo",
    "CreateViewSql": "SELECT  d .MoCode, c.StartDate, c.DueDate, a.iomodid, allocateid, a.cwhcode,a. cinvcode, cbatch, iinvexchrate, iquantity, inum, isTCl, \r\n                   '原功能' AS 来源, b.InvCode, f.cinvname as 材料名称,f2.cinvname as 成品名称, w.cWhName,m.ddate as 配料日期\r\nFROM      UFDATA_800_2018..pld_t AS a LEFT JOIN\r\n                   UFDATA_800_2018..mom_orderdetail AS b ON a.iomodid = b.MoDId LEFT JOIN\r\n                   UFDATA_800_2018..mom_morder AS c ON a.iomodid = c.MoDId LEFT JOIN\r\n                   UFDATA_800_2018..mom_order AS d ON c.MoId = d .MoId\r\nleft join UFDATA_800_2018..inventory  as f on a. cinvcode=f. cinvcode\r\nleft join UFDATA_800_2018..inventory  as f2 on b. invcode=f2. cinvcode\r\nleft join UFDATA_800_2018..Warehouse  as w on a.cwhcode=w.cwhcode\r\nleft join   UFDATA_800_2018..pld_m AS m on a.mid=m.id\r\nWHERE   b.MoDId IS NOT NULL\r\nUNION\r\nSELECT  d .MoCode, c.StartDate, c.DueDate, a.MODID, ALID, a.cwhcode, a.CINVCODE, CBATCH, 纯度, 有效数, 实物数, 是否填料, \r\n                   '新功能', b.InvCode, f.cinvname as 材料名称,f2.cinvname as 成品名称, w.cWhName,b.define37\r\n\r\n\r\nFROM      UFDATA_800_2018..plmx AS a LEFT JOIN\r\n                   UFDATA_800_2018..mom_orderdetail AS b ON a.modid = b.MoDId LEFT JOIN\r\n                   UFDATA_800_2018..mom_morder AS c ON a.modid = c.MoDId LEFT JOIN\r\n                   UFDATA_800_2018..mom_order AS d ON c.MoId = d .MoId\r\nleft join UFDATA_800_2018..inventory  as f on a. cinvcode=f. cinvcode\r\nleft join UFDATA_800_2018..inventory  as f2 on b. invcode=f2. cinvcode\r\nleft join UFDATA_800_2018..Warehouse  as w on a.cwhcode=w.cwhcode\r\nWHERE   a.MODID NOT IN\r\n                       (SELECT  iomodid\r\n                        FROM       UFDATA_800_2018..pld_t) AND b.MoDId IS NOT NULL",
