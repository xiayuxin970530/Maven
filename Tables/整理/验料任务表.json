  "ExternalDatabaseInfo": {
    "ConnectionId": "pld",
    "SourceTableName": "验料任务表",
    "SourceTableSchema": "dbo",
    "CreateViewSql": "SELECT  a.invcode, a.cInvCode, a.cBatch, a.cBarCode, b.dDate, b.dVeriDate, c.barcode, b.cCode, \r\n                   CASE WHEN a.cBarCode LIKE 'A%' THEN '唯一码' WHEN a.cBarCode LIKE '||st64|%' THEN '验行码' ELSE '批次码' END AS 验货方式,\r\n                    o.MoCode, t.MoLotCode, a.AutoID, a.ID, m.MoDId, m.AllocateId, ma.cbsysbarcode, ISNULL(y.负责部门, N'通用') \r\n                   AS 负责部门, y.属性, a.iQuantity, ISNULL(a.iNum, a.iQuantity) AS 实物数\r\nFROM      UFDATA_800_2018.dbo.rdrecords11 AS a LEFT OUTER JOIN\r\n                   UFDATA_800_2018.dbo.rdrecord11 AS b ON a.ID = b.ID LEFT OUTER JOIN\r\n                   dbo.材料扫码记录 AS c ON a.AutoID = c.outdid LEFT OUTER JOIN\r\n                   UFDATA_800_2018.dbo.mom_moallocate AS m ON a.iMPoIds = m.AllocateId LEFT OUTER JOIN\r\n                   UFDATA_800_2018.dbo.mom_orderdetail AS t ON m.MoDId = t.MoDId LEFT OUTER JOIN\r\n                   UFDATA_800_2018.dbo.mom_order AS o ON t.MoId = o.MoId LEFT OUTER JOIN\r\n                   UFDATA_800_2018.dbo.MaterialAppVouchs AS ma ON a.iMaIDs = ma.AutoID LEFT OUTER JOIN\r\n                   dbo.存货验料属性 AS y ON a.cInvCode = y.存货编码\r\nWHERE   (a.cBarCode IS NOT NULL) AND (b.dVeriDate IS NULL) AND (a.cDefine33 IS NULL) AND (b.cSource = '领料申请单') AND \r\n                   (ISNULL(y.属性, 0) <> '液体')",
    "UserSetQueryColumns": [
      "AutoID"
    ],
