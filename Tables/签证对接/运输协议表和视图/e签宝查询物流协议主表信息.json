  "ExternalDatabaseInfo": {
    "AllowModifySchema": true,
    "ConnectionId": "pld",
    "SourceTableName": "e签宝查询物流协议主表信息",
    "SourceTableSchema": "dbo",
    "CreateViewSql": "SELECT   TOP (1) PERCENT 任务主表ID, 来源类型, 业务日期 AS 发货日期, 承运方, (CASE WHEN 交付客户 IS NULL \r\n                THEN '无' WHEN 交付客户 = ' ' THEN '无' ELSE 交付客户 END) AS 客户名称, (CASE WHEN 我方签署人 IS NULL \r\n                THEN '无' WHEN 我方签署人 = ' ' THEN '无' ELSE 我方签署人 END) AS 我方签署人, \r\n                (CASE WHEN 我方签署手机号 IS NULL THEN '无' WHEN 我方签署手机号 = ' ' THEN '无' ELSE 我方签署手机号 END) \r\n                AS 我方签署手机号, (CASE WHEN 交付地址 IS NULL THEN '无' WHEN 交付地址 = ' ' THEN '无' ELSE 交付地址 END) \r\n                AS 客户地址, (CASE WHEN 对方签署人 IS NULL THEN '无' WHEN 对方签署人 = ' ' THEN '无' ELSE 对方签署人 END) \r\n                AS 联系人, (CASE WHEN 对方签署手机 IS NULL \r\n                THEN '无' WHEN 对方签署手机 = ' ' THEN '无' ELSE 对方签署手机 END) AS 收货联系人手机, '无' AS 备注, 签章内容, \r\n                合计数量, 合计运费, 法人1, 法人手机1, 法人2, 法人手机2, 法人3, 法人手机3, 车号, 托运方\r\nFROM      dbo.任务主表\r\nWHERE   (任务主表ID NOT IN\r\n                    (SELECT   任务主表id\r\n                     FROM      dbo.签章任务\r\n                     WHERE   (任务类型 = '物流运输协议'))) AND (承运方 <> '自提') AND (来源类型 = '物流运输协议') AND \r\n                (承运方 <> '快递公司') AND (承运方 <> '其他') AND (任务主表ID IN\r\n                    (SELECT   任务主表id\r\n                     FROM      dbo.任务子表))\r\nORDER BY 任务主表ID",
    "ViewInfo": {
      "ViewName": "e签宝查询物流协议主表信息",
      "IsUpdatable": true,
      "QueryColumns": []
    }
  },
