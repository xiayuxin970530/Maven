  "ExternalDatabaseInfo": {
    "ConnectionId": "pld",
    "SourceTableName": "扫码任务",
    "SourceTableSchema": "dbo",
    "CreateViewSql": "SELECT   a.pickid, a.pcinvcode AS dlcinvcod, a.contentqty AS 拣货总数, a.仓库 AS cwhcode, a.FGC_CreateDate AS pdate, \r\n                c.carcode, '' AS mark, '' AS code, a.FGC_Creator AS maker, ISNULL(b.扫码总数, 0) AS 已扫码, \r\n                a.contentqty - ISNULL(b.扫码总数, 0) AS 待扫码, a.发货单号 AS cDLCode, a.客户 AS 客户编码, a.客户 AS 客户名称, \r\n                a.contentID, a.ctbbatch, a.ctposcode, a.存货名称, a.存货规格, a.标识, c.code AS 流水号\r\nFROM      dbo.任务绑定明细 AS a LEFT OUTER JOIN\r\n                dbo.扫码结果汇总 AS b ON a.contentID = b.contentid LEFT OUTER JOIN\r\n                dbo.PICK AS c ON a.pickid = c.pickID\r\nWHERE   (ISNULL(a.标识出库比例, 0) < '1')",
    "UserSetQueryColumns": [
      "PICKID"
    ],
    "ViewInfo": {
      "ViewName": "扫码任务",
      "IsUpdatable": true,
      "QueryColumns": [
        "PICKID"
      ]
    }
