  "ExternalDatabaseInfo": {
    "ConnectionId": "pld",
    "SourceTableName": "货位存量表U8",
    "SourceTableSchema": "dbo",
    "CreateViewSql": "SELECT   MAX(CS.Autoid) AS autoid, CS.cWhCode, MAX(W.cWhName) AS cWhName, CS.cPosCode, \r\n                MAX(UFDATA_800_2018.dbo.Position.cPosName) AS cPosName, CS.cInvCode, CS.cBatch, ISNULL(CS.cvmivencode, N'') \r\n                AS cvmivencode, MAX(v1.cVenAbbName) AS cvmivenname, ISNULL(MAX(CS.dMadeDate), GETDATE()) AS dMDate, \r\n                MAX(CS.iMassDate) AS iMassDate, MAX(CS.cMassUnit) AS cMassUnit, MAX(ISNULL(E.EnumName, N'')) \r\n                AS cMassUnitName, MAX(CS.dVDate) AS dVDate, SUM(CS.iQuantity) AS iQuantity, SUM(CS.inum) AS iNum, \r\n                MAX(I.cInvAddCode) AS cInvAddCode, MAX(I.cInvName) AS cInvName, CS.cFree1, CS.cFree2, CS.cFree3, CS.cFree4, \r\n                CS.cFree5, CS.cFree6, CS.cFree7, CS.cFree8, CS.cFree9, CS.cFree10, MAX(v2.EnumName) AS 有效期推算方式, \r\n                MAX(CS.cExpirationdate) AS 有效期至, MAX(I.cInvStd) AS cInvStd, MAX(I.cInvCCode) AS cInvCCode, \r\n                MAX(UFDATA_800_2018.dbo.InventoryClass.cInvCName) AS cInvCName, 'ZT800' AS ZTCODE\r\nFROM      UFDATA_800_2018.dbo.InvPositionSum AS CS WITH (nolock) LEFT OUTER JOIN\r\n                UFDATA_800_2018.dbo.Warehouse AS W WITH (nolock) ON W.cWhCode = CS.cWhCode LEFT OUTER JOIN\r\n                UFDATA_800_2018.dbo.Inventory AS I ON CS.cInvCode = I.cInvCode LEFT OUTER JOIN\r\n                UFDATA_800_2018.dbo.Position ON CS.cPosCode = UFDATA_800_2018.dbo.Position.cPosCode LEFT OUTER JOIN\r\n                UFDATA_800_2018.dbo.InventoryClass ON \r\n                UFDATA_800_2018.dbo.InventoryClass.cInvCCode = I.cInvCCode LEFT OUTER JOIN\r\n                UFDATA_800_2018.dbo.v_aa_enum AS E WITH (nolock) ON E.EnumCode = CONVERT(nvarchar, CS.cMassUnit) AND \r\n                E.EnumType = N'ST.MassUnit' LEFT OUTER JOIN\r\n                UFDATA_800_2018.dbo.Vendor AS v1 ON CS.cvmivencode = v1.cVenCode LEFT OUTER JOIN\r\n                UFDATA_800_2018.dbo.v_aa_enum AS v2 ON v2.EnumCode = ISNULL(CS.iExpiratDateCalcu, 0) AND \r\n                v2.EnumType = N'SCM.ExpiratDateCalcu'\r\nWHERE   (CS.kfsx = 1)\r\nGROUP BY CS.cWhCode, CS.cPosCode, CS.cInvCode, CS.cBatch, ISNULL(CS.cvmivencode, N''), CS.cFree1, CS.cFree2, CS.cFree3, \r\n                CS.cFree4, CS.cFree5, CS.cFree6, CS.cFree7, CS.cFree8, CS.cFree9, CS.cFree10",
    "UserSetQueryColumns": [
      "autoid"
    ],
    "ViewInfo": {
      "ViewName": "货位存量表U8",
      "IsUpdatable": true,
      "QueryColumns": [
        "autoid"
      ]
    }
  },
