  "ExternalDatabaseInfo": {
    "ConnectionId": "pld",
    "SourceTableName": "e签宝对账单动态表格",
    "SourceTableSchema": "dbo",
    "CreateViewSql": "SELECT   'true' AS insertRow, CONVERT(varchar, DATEPART(yy, dbo.任务子表.日期)) + '年' + CONVERT(varchar, DATEPART(mm, \r\n                dbo.任务子表.日期)) + '月' + CONVERT(varchar, DATEPART(dd, dbo.任务子表.日期)) + '日' AS column1, \r\n                i.cInvName AS column2, i.cInvStd AS column3, CAST(dbo.任务子表.交付确认数量 AS decimal(18, 4)) AS column4, \r\n                CAST(dbo.任务子表.交付确认单价 AS decimal(18, 4)) AS column5, CAST(dbo.任务子表.金额 AS decimal(18, 2)) \r\n                AS column6, CAST(ISNULL(dbo.任务子表.回款金额, 0) AS decimal(18, 2)) AS column7, \r\n                CAST(ISNULL(dbo.任务子表.当前行余额, 0) AS decimal(18, 2)) AS column8, dbo.任务子表.来源备注 AS column9, \r\n                dbo.任务子表.回款类型 AS column10, dbo.任务子表.任务主表id, dbo.任务子表.任务子表ID\r\nFROM      dbo.任务子表 LEFT OUTER JOIN\r\n                dbo.inventory AS i ON i.cInvCode = dbo.任务子表.存货编码",
