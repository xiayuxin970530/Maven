  "ExternalDatabaseInfo": {
    "AllowModifySchema": true,
    "ConnectionId": "pld",
    "SourceTableName": "e签宝查询客户回执主表信息",
    "SourceTableSchema": "dbo",
    "CreateViewSql": "SELECT   TOP (1) a.任务主表ID, a.来源类型, a.业务日期 AS 发货日期, (CASE WHEN 交付客户 IS NULL \r\n                THEN '无' WHEN 交付客户 = ' ' THEN '无' ELSE 交付客户 END) AS 客户名称, (CASE WHEN 交付地址 IS NULL \r\n                THEN '无' WHEN 交付地址 = ' ' THEN '无' ELSE 交付地址 END) AS 交付地址, (CASE WHEN 对方签署人 IS NULL \r\n                THEN '无' WHEN 对方签署人 = ' ' THEN '无' ELSE 对方签署人 END) AS 联系人, (CASE WHEN 对方签署手机 IS NULL \r\n                THEN '无' WHEN 对方签署手机 = ' ' THEN '无' ELSE 对方签署手机 END) AS 收货联系人手机, a.客户回执法人, \r\n                a.客户回执法人手机号, a.我方签署人, (CASE WHEN b.手机号码 IS NULL \r\n                THEN 我方签署手机号 WHEN b.手机号码 = '' THEN 我方签署手机号 ELSE 我方签署手机号 END) AS 我方签署手机, \r\n                a.crm单号, CONVERT(varchar, DATEPART(yy, a.业务日期)) + '年' + CONVERT(varchar, DATEPART(mm, a.业务日期)) \r\n                + '月' AS 年月\r\nFROM      dbo.任务主表 AS a LEFT OUTER JOIN\r\n                dbo.业务员联系方式 AS b ON a.我方签署人 = b.业务经理\r\nWHERE   (CAST(a.业务日期 AS date) = CAST(GETDATE() AS date)) AND (a.任务主表ID NOT IN\r\n                    (SELECT   任务主表id\r\n                     FROM      dbo.签章任务\r\n                     WHERE   (任务类型 = '客户回执'))) AND (a.来源类型 = '客户回执') AND\r\n                    ((SELECT   COUNT(*) AS 数据\r\n                      FROM      dbo.任务子表\r\n                      WHERE   (任务主表id = a.任务主表ID)) <> 0)\r\nORDER BY a.任务主表ID",
    "ViewInfo": {
      "ViewName": "e签宝查询客户回执主表信息",
      "IsUpdatable": true,
      "QueryColumns": []
    }
  },
