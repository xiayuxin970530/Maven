    "SourceTableName": "特殊仓库可用量汇总",
    "SourceTableSchema": "dbo",
    "CreateViewSql": "SELECT  TOP (100) PERCENT cInvCode, cInvName, cInvStd, SUM(iquantity) AS 可用数量, SUM(inum) AS 可用件数, SUM(实际库存) \r\n                   AS 实际数量, SUM(实际件数) AS 实际件数, SUM(合计占用) AS 合计占用, SUM(待入库数量) AS 合计待入库, \r\n                   SUM(冻结数量) AS 合计冻结数量, SUM(冻结件数) AS 合计冻结件数\r\nFROM      dbo.净可用库存\r\nWHERE   (cWhCode IN\r\n                       (SELECT  仓库编码\r\n                        FROM       dbo.配料可用仓库\r\n                        WHERE    (ISNULL(虚拟库存特殊可用, 0) = 1)))\r\nGROUP BY cInvCode, cInvName, cInvStd\r\nORDER BY 合计占用",
