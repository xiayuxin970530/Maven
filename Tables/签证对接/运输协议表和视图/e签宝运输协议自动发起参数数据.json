  "ExternalDatabaseInfo": {
    "ConnectionId": "pld",
    "SourceTableName": "e签宝运输协议自动发起参数数据",
    "SourceTableSchema": "dbo",
    "CreateViewSql": "SELECT   a.签章任务ID, a.任务主表id, a.我方签署人, (CASE WHEN b.手机号码 IS NULL \r\n                THEN 我方电话 WHEN b.手机号码 = '' THEN 我方电话 ELSE 我方电话 END) AS 我方电话, a.对方签署人, a.对方电话, \r\n                ISNULL(a.签章内容, N'没有明细') AS 签章内容, a.匹配模板id, a.匹配模板名称, a.任务类型, a.承运方, a.客户名称, \r\n                a.客户地址, CONVERT(varchar, DATEPART(yy, a.发货日期)) + '年' + CONVERT(varchar, DATEPART(mm, a.发货日期)) \r\n                + '月' + CONVERT(varchar, DATEPART(dd, a.发货日期)) + '日' AS 发货日期, ISNULL(a.备注, N'无') AS 备注, \r\n                ISNULL(a.数量合计, 0) AS 数量合计, ISNULL(a.运费合计, 0) AS 运费合计, a.客户回执法人, a.客户回执法人手机, \r\n                a.签收人, a.签收人电话, a.crm单号, a.接口发起状态, a.公司类型, CONVERT(varchar, DATEPART(yy, a.发货日期)) \r\n                + '年' + CONVERT(varchar, DATEPART(mm, a.发货日期)) + '月' AS 年月\r\nFROM      dbo.签章任务 AS a LEFT OUTER JOIN\r\n                dbo.业务员联系方式 AS b ON a.我方签署人 = b.业务经理\r\nWHERE   (a.任务类型 = '客户回执') AND (a.接口发起状态 IS NULL) AND (a.我方签署人 <> '吕鹏')",
    "ViewInfo": {
      "ViewName": "e签宝运输协议自动发起参数数据",
      "IsUpdatable": true,
      "QueryColumns": []
    }
  },
