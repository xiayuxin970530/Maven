  "ExternalDatabaseInfo": {
    "AllowModifySchema": true,
    "ConnectionId": "pld",
    "SourceTableName": "半成品虚拟库存",
    "SourceTableSchema": "dbo",
    "CreateViewSql": "SELECT  p.cwhcode AS 仓库编码, w.cWhName AS 仓库名称, p.CINVCODE AS 编码, p.CBATCH AS 批次, i.cInvName AS 名称, \r\n                   i.cInvStd AS 规格, p.有效数 - p.材料出库数 AS 占用数量, c.iQuantity AS 现存数量, \r\n                   c.fStopQuantity AS 其中冻结数量\r\nFROM      UFDATA_800_2018.dbo.PLMX AS p LEFT OUTER JOIN\r\n                   UFDATA_800_2018.dbo.Warehouse AS w ON p.cwhcode = w.cWhCode LEFT OUTER JOIN\r\n                   UFDATA_800_2018.dbo.Inventory AS i ON i.cInvCode = p.CINVCODE LEFT OUTER JOIN\r\n                   UFDATA_800_2018.dbo.CurrentStock AS c ON c.cInvCode = p.CINVCODE AND p.CBATCH = c.cBatch\r\nWHERE   (p.生产订单状态 = 3) AND (p.材料出库数 <= 0) AND (p.cwhcode IN (113, 114, 130, 131, 205, 206)) AND (p.是否成品 = '是')",
