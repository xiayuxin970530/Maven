  "ExternalDatabaseInfo": {
    "ConnectionId": "pld",
    "SourceTableName": "货位存量",
    "SourceTableSchema": "dbo",
    "CreateViewSql": "SELECT  a.autoid, a.cWhCode, a.cWhName, a.cPosCode, a.cPosName, a.cInvCode, a.cBatch, a.cvmivencode, a.cvmivenname, \r\n                   a.dMDate, a.iMassDate, a.cMassUnit, a.cMassUnitName, a.dVDate, a.iQuantity - ISNULL(b.扫描汇总, 0) AS iQuantity, \r\n                   a.iQuantity AS 现存量, a.iNum, a.cInvAddCode, a.cInvName, a.cFree1, a.cFree2, a.cFree3, a.cFree4, a.cFree5, a.cFree6, \r\n                   a.cFree7, a.cFree8, a.cFree9, a.cFree10, a.有效期推算方式, a.有效期至, a.cInvStd, a.cInvCCode, a.cInvCName\r\nFROM      dbo.货位存量表U8 AS a LEFT OUTER JOIN\r\n                   dbo.扫描货位批次汇总 AS b ON a.cInvCode = b.cinvcode AND b.invpostion = a.cPosCode LEFT OUTER JOIN\r\n                       (SELECT DISTINCT pcinvcode\r\n                        FROM       dbo.任务绑定明细\r\n                        WHERE    (ISNULL(标识出库比例, 0) <> 1)) AS d ON a.cInvCode = d.pcinvcode\r\nWHERE   (d.pcinvcode IS NOT NULL)",
    "UserSetQueryColumns": [
      "autoid"
    ],
    "ViewInfo": {
      "ViewName": "货位存量",
      "IsUpdatable": true,
      "QueryColumns": [
        "autoid"
      ]
    }
  },
