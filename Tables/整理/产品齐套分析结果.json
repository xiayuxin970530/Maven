  "ExternalDatabaseInfo": {
    "ConnectionId": "pld",
    "SourceTableName": "产品齐套分析结果",
    "SourceTableSchema": "dbo",
    "CreateViewSql": "SELECT  a.总需求, a.末级物料编码, i.cInvCode, b.合计可用, b.合计实际库存, b.合计占用 + ISNULL(bc.包材合计占用, 0) \r\n                   AS 合计占用, i.cInvName, i.cInvStd, ISNULL(b.合计可用, 0) - (ISNULL(b.合计占用, 0) + ISNULL(bc.包材合计占用, 0)) \r\n                   - ISNULL(a.总需求, 0) AS 在库差额, a.类型, b.合计待入库\r\nFROM      dbo.产品齐套分析汇总 AS a LEFT OUTER JOIN\r\n                   dbo.可用库存汇总表 AS b ON a.末级物料编码 = b.cInvCode LEFT OUTER JOIN\r\n                   UFDATA_800_2018.dbo.Inventory AS i ON a.末级物料编码 = i.cInvCode LEFT OUTER JOIN\r\n                   dbo.生产包材合计占用 AS bc ON bc.InvCode = a.末级物料编码",
