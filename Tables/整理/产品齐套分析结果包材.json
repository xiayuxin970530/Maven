  "ExternalDatabaseInfo": {
    "ConnectionId": "pld",
    "SourceTableName": "产品齐套分析结果包材",
    "SourceTableSchema": "dbo",
    "CreateViewSql": "SELECT  a.Free8 AS 母件名称, a.Free9 AS 母件规格, i.cInvStd AS 子件规格, i.cInvName AS 子件名称, a.InvCode AS 母件编码, \r\n                   (CASE WHEN a.BomType = 1 THEN '主BOM' WHEN a.BomType = 2 THEN '替代BOM' END) AS 替代标识, \r\n                   a.Version AS 成品版本, a.VersionEffDate AS 成品有效日期, c.InvCode AS 子件编码, c.BaseQtyD AS 子件分子, \r\n                   c.BaseQtyN AS 子件分母, 1 * c.BaseQtyN / c.BaseQtyD AS 子件用量, 1 * c.BaseQtyN / c.BaseQtyD AS 需求数, \r\n                   c.InvCode AS 末级物料编码, a.Version, a.VersionDesc AS 版本说明, c.BaseQtyN / c.BaseQtyD AS 基本用量, \r\n                   ISNULL(b.合计可用, 0) AS 合计可用, ISNULL(ISNULL(b.合计可用, 0) - ISNULL(bc.包材合计占用, 0) + ISNULL(j.合计占用, 0), \r\n                   0) AS 包材数量, bc.包材合计占用, ISNULL((ISNULL(b.合计可用, 0) - ISNULL(bc.包材合计占用, 0)) \r\n                   / (c.BaseQtyN / c.BaseQtyD), 0) AS 包材齐套分析, c.Remark, ISNULL(j.合计占用, 0) AS 合计占用\r\nFROM      UFDATA_800_2018.dbo.v_BOM_BomParent AS a LEFT OUTER JOIN\r\n                   UFDATA_800_2018.dbo.v_bom_opcomponent_rpt AS c ON a.BomId = c.BomId LEFT OUTER JOIN\r\n                   UFDATA_800_2018.dbo.Inventory AS i ON c.InvCode = i.cInvCode LEFT OUTER JOIN\r\n                   dbo.可用库存汇总表 AS b ON c.InvCode = b.cInvCode LEFT OUTER JOIN\r\n                   dbo.生产包材合计占用 AS bc ON bc.InvCode = c.InvCode LEFT OUTER JOIN\r\n                   dbo.净可用库存 AS j ON j.cInvCode = i.cInvCode\r\nGROUP BY a.Free8, a.Free9, i.cInvStd, i.cInvName, a.InvCode, a.Version, a.VersionEffDate, c.InvCode, c.BaseQtyD, c.BaseQtyN, \r\n                   c.InvCode, a.Version, a.VersionDesc, b.合计可用, bc.包材合计占用, j.合计占用, c.Remark, a.BomType",
    "ViewInfo": {
      "IsUpdatable": true,
      "QueryColumns": []
    }
  },
