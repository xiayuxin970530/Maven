  "ExternalDatabaseInfo": {
    "ConnectionId": "pld",
    "SourceTableName": "拣货汇总zt700",
    "SourceTableSchema": "dbo",
    "CreateViewSql": "SELECT  a.PICKID, a.dlcinvcod, SUM(a.dlqty) - SUM(ISNULL(a.出库数, 0)) AS 拣货总数, a.cwhcode, b.pdate, b.carcode, b.mark, \r\n                   b.code, b.maker, '发货编号' AS cdlcode, '客户编码' AS 客户编码, '客户名称' AS 客户名称, a.dlinvstd, 'a.cdlcode' AS Expr1, \r\n                   a.dlcivname, a.ztcode, a.标识\r\nFROM      dbo.PICKS AS a LEFT OUTER JOIN\r\n                   dbo.PICK AS b ON a.PICKID = b.pickID LEFT OUTER JOIN\r\n                   dbo.发货单不过滤zt700 AS c ON a.dlautoid = c.AutoID\r\nWHERE   (ISNULL(a.出库比例, 0) < '1') AND (a.ztcode = 'ZT700')\r\nGROUP BY a.PICKID, a.dlcinvcod, a.cwhcode, b.pdate, b.carcode, b.mark, b.code, b.maker, a.dlinvstd, a.dlcivname, a.ztcode, a.标识",
