  "ExternalDatabaseInfo": {
    "ConnectionId": "pld",
    "SourceTableName": "拣货建议汇总",
    "SourceTableSchema": "dbo",
    "CreateViewSql": "SELECT  a.PICKID, a.dlcinvcod, a.拣货总数 AS 拣货汇总, b.cPosCode, b.cPosName, b.iquantity AS 库存汇总, c.扫码总数, \r\n                   a.拣货总数 - c.扫码总数 AS 未扫码数, a.cwhcode, a.客户编码, a.cdlcode, a.客户名称, c.dlcode, c.dlcuscode\r\nFROM      dbo.拣货汇总 AS a LEFT OUTER JOIN\r\n                       (SELECT  cInvCode, cPosCode, cPosName, SUM(iQuantity) AS iquantity, cWhCode\r\n                        FROM       dbo.货位存量表\r\n                        GROUP BY cInvCode, cPosCode, cPosName, cWhCode) AS b ON a.dlcinvcod = b.cInvCode AND \r\n                   a.cwhcode = b.cWhCode LEFT OUTER JOIN\r\n                   dbo.扫码结果汇总 AS c ON a.PICKID = c.pickid AND a.dlcinvcod = c.cinvcode AND a.cwhcode = c.cwhcode AND \r\n                   a.cdlcode = c.dlcode AND a.客户编码 = c.dlcuscode",
