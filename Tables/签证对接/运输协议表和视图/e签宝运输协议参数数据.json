  "ExternalDatabaseInfo": {
    "AllowModifySchema": true,
    "ConnectionId": "pld",
    "SourceTableName": "e签宝运输协议参数数据",
    "SourceTableSchema": "dbo",
    "CreateViewSql": "SELECT   签章任务ID, 任务主表id, ISNULL(我方签署人, '夏') AS 我方签署人, ISNULL(我方电话, '18765192922') AS 我方电话, \r\n                对方签署人, 对方电话, ISNULL(签章内容, '没有明细') AS 签章内容, 匹配模板id, 匹配模板名称, 任务类型, 承运方, \r\n                客户名称, 客户地址, CONVERT(varchar(12), 发货日期, 111) AS 发货日期, ISNULL(备注, '无') AS 备注, ISNULL(数量合计, \r\n                0) AS 数量合计, ISNULL(运费合计, 0) AS 运费合计, 发运人1, 发运人手机号1, 发运人2, 发运人手机号2, 发运人3, \r\n                发运人手机号3, 法人1, 法人手机号1, 法人2, 法人手机号2, 法人3, 法人手机号3, 仓管1, 仓管手机人1, 仓管2, \r\n                仓管手机号2, 仓管3, 仓管手机号3, 车牌号, 托运方, 接口发起状态\r\nFROM      dbo.签章任务\r\nWHERE   (任务类型 = '物流运输协议') AND (接口发起状态 IS NULL OR\r\n                接口发起状态 = 7 OR\r\n                接口发起状态 = 3 OR\r\n                接口发起状态 = 5)",
