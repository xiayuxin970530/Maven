  "ExternalDatabaseInfo": {
    "ConnectionId": "pld",
    "SourceTableName": "e签宝对账单主表数据视图",
    "SourceTableSchema": "dbo",
    "CreateViewSql": "SELECT   dbo.任务主表.对账公司 AS 我方公司名称, CONVERT(varchar, DATEPART(yy, dbo.任务主表.对账开始日期)) \r\n                + '年' + CONVERT(varchar, DATEPART(mm, dbo.任务主表.对账开始日期)) + '月' + CONVERT(varchar, DATEPART(dd, \r\n                dbo.任务主表.对账开始日期)) + '日' AS 对账开始日期, CONVERT(varchar, DATEPART(yy, dbo.任务主表.对账结束日期)) \r\n                + '年' + CONVERT(varchar, DATEPART(mm, dbo.任务主表.对账结束日期)) + '月' + CONVERT(varchar, DATEPART(dd, \r\n                dbo.任务主表.对账结束日期)) + '日' AS 对账结束日期, YEAR(dbo.任务主表.对账结束日期) AS 年, \r\n                dbo.任务主表.交付客户, dbo.任务主表.应收账面余额, dbo.任务主表.应收账面余额大写, CONVERT(varchar, \r\n                DATEPART(yy, dbo.任务主表.对账结束日期)) + '年' + CONVERT(varchar, DATEPART(mm, dbo.任务主表.对账结束日期)) \r\n                + '月' + CONVERT(varchar, DATEPART(dd, dbo.任务主表.对账结束日期)) + '日' AS 日期1, \r\n                YEAR(dbo.任务主表.对账结束日期) + 1 AS 年度1, dbo.任务主表.预收款描述, \r\n                (CASE WHEN dbo.任务主表.预收款 = 0 THEN '0.00' ELSE CAST(dbo.任务主表.预收款 AS decimal(18, 2)) END) AS 预收款, \r\n                dbo.任务主表.预收款大写, CONVERT(varchar, DATEPART(yy, dbo.任务主表.对账结束日期)) + '年' + CONVERT(varchar, \r\n                DATEPART(mm, dbo.任务主表.对账结束日期)) + '月' + CONVERT(varchar, DATEPART(dd, dbo.任务主表.对账结束日期)) \r\n                + '日' AS 日期2, YEAR(dbo.任务主表.对账结束日期) AS 年度2, dbo.任务主表.预收余额描述, \r\n                (CASE WHEN dbo.任务主表.预收款余额 = 0 THEN '0.00' ELSE CAST(dbo.任务主表.预收款余额 AS decimal(18, 2)) END) \r\n                AS 预收款余额, dbo.任务主表.预收款余额大写, CONVERT(varchar, DATEPART(yy, dbo.任务主表.对账结束日期)) \r\n                + '年' + CONVERT(varchar, DATEPART(mm, dbo.任务主表.对账结束日期)) + '月' + CONVERT(varchar, DATEPART(dd, \r\n                dbo.任务主表.对账结束日期)) + '日' AS 日期3, CAST(dbo.任务主表.应收余额 AS decimal(18, 2)) AS 应收余额, \r\n                dbo.任务主表.应收余额大写, (CASE WHEN b.ccdefine1 IS NULL \r\n                THEN '夏雨鑫' WHEN b.ccdefine1 = '' THEN '夏雨鑫' ELSE b.ccdefine1 END) AS 经办人, \r\n                (CASE WHEN b.ccdefine2 IS NULL \r\n                THEN '18765192922' WHEN b.ccdefine2 = '' THEN '18765192922' ELSE b.ccdefine2 END) AS 经办人手机号, \r\n                CONVERT(varchar, DATEPART(yy, dbo.任务主表.业务日期)) + '年' + CONVERT(varchar, DATEPART(mm, \r\n                dbo.任务主表.业务日期)) + '月' + CONVERT(varchar, DATEPART(dd, dbo.任务主表.业务日期)) + '日' AS 发货日期, \r\n                dbo.任务主表.任务主表ID, dbo.任务主表.业务员名称, dbo.任务主表.业务员电话, dbo.任务主表.对账类型, \r\n                dbo.任务主表.公司类型\r\nFROM      dbo.任务主表 LEFT OUTER JOIN\r\n                dbo.获取700和800中对账单和发货回执单接收人 AS b ON dbo.任务主表.交付客户 = b.cCusName",
    "ViewInfo": {
      "ViewName": "e签宝对账单主表数据视图",
      "IsUpdatable": true,
      "QueryColumns": []
    }
  },
