    "CreateViewSql": "SELECT   a.PICKsID, a.dlqty, CAST(ISNULL(a.dlqty, 0) - ISNULL(b.核销总数, 0) AS decimal(18, 4)) AS 核销余额\r\nFROM      dbo.PICKS AS a LEFT OUTER JOIN\r\n                    (SELECT   picksid, SUM(ISNULL(核销数量, '0')) AS 核销总数\r\n                     FROM      dbo.发货拣货核销表\r\n                     GROUP BY picksid) AS b ON a.PICKsID = b.picksid\r\nWHERE   (ISNULL(a.出库比例, 0) <> 1)",
