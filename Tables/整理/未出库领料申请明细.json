  "ExternalDatabaseInfo": {
    "ConnectionId": "pld",
    "SourceTableName": "未出库领料申请明细",
    "SourceTableSchema": "dbo",
    "CreateViewSql": "SELECT  a.cmocode AS 生产单号, a.iMPoIds, a.AutoID, a.cInvCode, a.iQuantity, a.iinvexchrate, ISNULL(a.iNum, a.iQuantity) AS inum, \r\n                   a.fOutQuantity, a.fOutNum, a.iQuantity - ISNULL(a.fOutQuantity, 0) AS 未出库折百, a.cBatch AS 出库批次, ISNULL(a.iNum, \r\n                   a.iQuantity) - ISNULL(a.fOutNum, ISNULL(a.fOutQuantity, 0)) AS 未出库实物, b.iQuantity AS 库存折百, \r\n                   b.iNum AS 库存实物, CASE WHEN isnull(b.iNum, 0) != '0' THEN CAST(b.iQuantity / b.iNum AS decimal(18, 4)) \r\n                   ELSE 1 END AS 库存换算率, a.cWhCode, b.cWhCode AS Expr1, a.ID, a.cbsysbarcode, a.iQuantity - ISNULL(d.合计扫描, 0) \r\n                   AS 未扫描折百, CAST((a.iQuantity - ISNULL(d.合计扫描, 0)) / ISNULL(a.iinvexchrate, 1) AS decimal(18, 4)) AS 未扫码实物, \r\n                   p.袋数, p.单袋重, p.余数\r\nFROM      UFDATA_800_2018.dbo.MaterialAppVouchs AS a LEFT OUTER JOIN\r\n                   UFDATA_800_2018.dbo.CurrentStock AS b ON a.cInvCode = b.cInvCode AND ISNULL(a.cBatch, 0) = ISNULL(b.cBatch, 0) \r\n                   AND b.iQuantity > 0 AND a.cWhCode = b.cWhCode LEFT OUTER JOIN\r\n                       (SELECT  imasid, SUM(出库折百数量) AS 合计扫描\r\n                        FROM       dbo.材料扫码记录\r\n                        GROUP BY imasid) AS d ON a.AutoID = d.imasid LEFT OUTER JOIN\r\n                   UFDATA_800_2018.dbo.PLMX AS p ON a.AutoID = p.LLSQDID LEFT OUTER JOIN\r\n                   UFDATA_800_2018.dbo.MaterialAppVouch AS m ON a.ID = m.ID\r\nWHERE   (ISNULL(a.fOutQuantity, 0) < a.iQuantity) AND (a.ID IN\r\n                       (SELECT  LLSQMID\r\n                        FROM       UFDATA_800_2018.dbo.PLMX)) AND (m.cCloser IS NULL)",
    "UserSetQueryColumns": [
      "AutoID"
    ],
